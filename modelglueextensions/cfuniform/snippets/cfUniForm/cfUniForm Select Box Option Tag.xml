<?xml version="1.0" encoding="utf-8"?>
<snippet filetemplate="false" extension="cfm">
<name>cfUniForm Select Box Option Tag</name>
<help>Inserts a cfUniForm option tag</help>
<starttext><![CDATA[					<uform:option display="$${Display Text}" value="$${Value}" isSelected="$${Is Selected?:true|false}" />
]]></starttext>
<endtext><![CDATA[]]></endtext>
</snippet>